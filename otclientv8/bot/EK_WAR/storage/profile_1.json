  "ingame_hotkeys": "local holdMwallHotkey = 'CapsLock'\nlocal holdMwallText = 'Hold MWall'\nlocal holdMwallTextCol = 'yellow'\nlocal holdMwallTiles = {}\n\nonKeyPress(function(keys)\n  if keys == holdMwallHotkey then\n    local tile = getTileUnderCursor()\n    local currTile = table.find(holdMwallTiles, tile)\n    if currTile then\n      holdMwallTiles[currTile]:setText('')\n      table.remove(holdMwallTiles, currTile)\n    else\n      table.insert(holdMwallTiles, tile)\n      holdMwallTiles[#holdMwallTiles]:setText(holdMwallText, holdMwallTextCol)\n    end\n  end\nend)\n\nmacro(10, function()\n  for _, holdMwallTile in pairs(holdMwallTiles) do\n    local isWall = holdMwallTile:getTopThing():getId() == 2129 or holdMwallTile:getTopThing():getId() == 2130\n    local canShootWall = getDistanceBetween(holdMwallTile:getPosition(), player:getPosition())\n    if isWall and canShootWall and holdMwallTile:isWalkable() then\n      useWith(3180, holdMwallTile:getTopUseThing())\n      return\n    end\n  end\nend)\n\nlocal healHotkey = 'F1'\nlocal healSpell = \"exana mort\"\nlocal healPotID = 7643\n\ng_keyboard.bindKeyDown(healHotkey, function()\n  cast(healSpell)\n  useWith(healPotID, player)\nend)\n\n\n--\n--          [[[[[[[[[[[[[      ]]]]]]]]]]]]\n--          [[[[[[[[[[ MAX PUSH ]]]]]]]]]]\n--          [[[[[[[[[[[[[      ]]]]]]]]]]]]\n--\n\n\nlocal pushMaxState = {\n  tileTo = nil,\n  target = nil,\n  posFrom = nil,\n  state = 0,\n  lastPush = 0,\n}\n\nlocal pushMaxCfg = {\n  pushDelay = 690,\n  mwPushDelay = 400,\n  fieldId = 3164,\n}\n\nfunction resetPushMaxState()\n  if pushMaxState.posFrom then \n    g_map.getTile(pushMaxState.posFrom):setText('')\n  end\n  if pushMaxState.tileTo then \n    pushMaxState.tileTo:setText('') \n  end\n  pushMaxState.tileTo = nil\n  pushMaxState.target = nil\n  pushMaxState.state = 0\nend\n\nfunction setState(tile)\n  if pushMaxState.state == 0 then\n    local creature = tile:getCreatures() and tile:getCreatures()[1]\n    if creature and creature:isPlayer() then\n      pushMaxState.target = creature\n      -- used only to clear ui\n      pushMaxState.posFrom = creature:getPosition()\n      tile:setText('FROM')\n    else\n      tile:setText('NOT PLAYER')\n      return\n    end\n  end\n  if pushMaxState.state == 1 then\n    pushMaxState.tileTo = tile\n    tile:setText('HERE')\n  end\n  pushMaxState.state = pushMaxState.state + 1\n    print(\"state is \" .. pushMaxState.state)\nend\n\n-- state manager\nonKeyPress(function(keys)\n  if keys:lower() == \"`\" then\n    if pushMaxState.state > 1 then\n      resetPushMaxState()\n      return\n    end\n    local tile = getTileUnderCursor(mousePos)\n    setState(tile)\n  end\nend)\n\nonCreaturePositionChange(function(creature,newPos,oldPos)\n  if creature == player or creature == pushMaxState.target then \n    resetPushMaxState()\n    vBot.isUsing = false\n  end\nend)\n\n\nlocal function push(target,destPos, withField)\n  pushMaxState.lastPush = withField and now + math.max(2*pushMaxCfg.pushDelay, 2*pushMaxCfg.mwPushDelay)  or now + math.max(pushMaxCfg.pushDelay, pushMaxCfg.mwPushDelay)\n  g_game.move(target, destPos)\nend\n\nmacro(1, function()\n  if pushMaxState.state < 2 then return end\n  if now < pushMaxState.lastPush then return end\n  local isTargetPushable = getDistanceBetween(pushMaxState.target:getPosition(), player:getPosition()) == 1\n  if not isTargetPushable then return end\n  local isDestTileMwalled = not pushMaxState.tileTo:isWalkable() and pushMaxState.tileTo:getTopUseThing():getId() == 2129\n  local shouldUseRune = not pushMaxState.target:getTile():getTopUseThing():isNotMoveable()\n  vBot.isUsing = true\n  if isDestTileMwalled then\n    local pushDelay = shouldUseRune and 2*pushMaxCfg.mwPushDelay or pushMaxCfg.mwPushDelay\n    if pushMaxState.tileTo:getTimer() > pushDelay then \n      return \n    end\n    if shouldUseRune then\n      print(\"pushing... need to use rune \")\n      useWith(pushMaxCfg.fieldId, pushMaxState.target)\n      push(pushMaxState.target,pushMaxState.tileTo:getPosition(),true)\n      return\n    end\n    push(pushMaxState.target, pushMaxState.tileTo:getPosition())\n    return\n  end\n\n  if shouldUseRune then\n        print(\"need to use rune... \")\n    useWith(pushMaxCfg.fieldId, pushMaxState.target)\n    push(pushMaxState.target,pushMaxState.tileTo:getPosition(),true)\n    return\n  end\n      push(pushMaxState.target,pushMaxState.tileTo:getPosition())\n  vBot.isUsing = false\nend)\n\n\n\n\n\nlocal friendManaState = false\nlocal friendName = \"Tarambuniarz\"\n\nonScreen= macro(100,\"friend OS\", function()\n    local spectators = getSpectators(pos(), false)\n    for i, spec in pairs(spectators) do\n        if spec:getName() == friendName then\n          if getDistanceBetween(pos(), spec:getPosition()) < 5 and friendManaState then\n            return CaveBot.setOn()\n          end\n        end\n    end\n    return CaveBot.setOff()\nend)\n\n\n\n\n\nonTalk(function(name, level, mode, text, channelId, pos)\n    if name ~= \"Tarambuniarz\" then return end\n    if text == \"start\" then\n        friendManaState = true\n        CaveBot.setOn()\n    end\n    if text == \"stop\" then\n        friendManaState = false\n        CaveBot.setOff()\n    end\nend)\n\n\n\n",
